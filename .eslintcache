modal-title-vcenter\">\n            Search Results\n          </Modal.Title>\n        </Modal.Header>\n        <Modal.Body> \n            <ul>\n                {mappedResults}\n            </ul>\n        </Modal.Body>\n        <Modal.Footer>\n          <button onClick={onHide}>Close</button>\n        </Modal.Footer>\n      </Modal>\n    );\n  }\n\nexport default SearchResultsModal","/Users/anthonyrubbo/development/we_pick/we_pick_frontend/src/components/items/UserSearch.js",[],"/Users/anthonyrubbo/development/we_pick/we_pick_frontend/src/components/items/SigninModal.js",[],"/Users/anthonyrubbo/development/we_pick/we_pick_frontend/src/components/items/SignupModal.js",[],"/Users/anthonyrubbo/development/we_pick/we_pick_frontend/src/components/unused_pages/Login.js",[],["88","89"],"/Users/anthonyrubbo/development/we_pick/we_pick_frontend/src/components/unused_pages/Signup.js",[],"/Users/anthonyrubbo/development/we_pick/we_pick_frontend/src/components/items/EditModal.js",[],"/Users/anthonyrubbo/development/we_pick/we_pick_frontend/src/components/items/CompareModal.js",["90"],"import Modal from 'react-bootstrap/Modal'\nimport {Link} from \"react-router-dom\"\n// import {useState} from \"react\"\n\nfunction CompareModal (props) {\n\n    const {onHide, matchedMovies, setDetailsMovieId} = props\n\n    function onLinkClick(movId) {\n      localStorage.setItem('id', movId);\n        // setDetailsMovieId(movId)\n        onHide()\n    }\n\n    const showMatchedMovies = matchedMovies ? matchedMovies.map(mov => {\n        return <div className=\"matched-movies\">\n            <Link to={`/movie/${mov.id}`} onClick={() => onLinkClick(mov.id)} key={mov.title} >{mov.title} {mov.release_date ? `| ${mov.release_date.slice(0,4)}` : null}</Link>\n        </div>\n    }) : null\n\n    return (\n        <Modal\n        {...props}\n        size=\"lg\"\n        aria-labelledby=\"contained-modal-title-vcenter\"\n        centered\n      >\n        <Modal.Header>\n          <Modal.Title id=\"contained-modal-title-vcenter\">\n          You've got some matches!\n          </Modal.Title>\n        </Modal.Header>\n        <Modal.Body> \n            <div className=\"movie-matches\">\n              {showMatchedMovies}\n            </div>\n        </Modal.Body>\n        <Modal.Footer>\n          <button onClick={onHide}>Close</button>\n        </Modal.Footer>\n      </Modal>\n    )\n}\n\nexport default CompareModal","/Users/anthonyrubbo/development/we_pick/we_pick_frontend/src/components/items/TrailerModal.js",[],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":67,"column":14,"nodeType":"97","messageId":"98","endLine":67,"endColumn":29},{"ruleId":"95","severity":1,"message":"99","line":1,"column":21,"nodeType":"97","messageId":"98","endLine":1,"endColumn":29},{"ruleId":"95","severity":1,"message":"100","line":5,"column":35,"nodeType":"97","messageId":"98","endLine":5,"endColumn":52},{"ruleId":"91","replacedBy":"101"},{"ruleId":"93","replacedBy":"102"},{"ruleId":"95","severity":1,"message":"100","line":7,"column":35,"nodeType":"97","messageId":"98","endLine":7,"endColumn":52},"no-native-reassign",["103"],"no-negated-in-lhs",["104"],"no-unused-vars","'onGenreDropdown' is defined but never used.","Identifier","unusedVar","'Redirect' is defined but never used.","'setDetailsMovieId' is assigned a value but never used.",["103"],["104"],"no-global-assign","no-unsafe-negation"]